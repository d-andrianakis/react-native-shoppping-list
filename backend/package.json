{
  "name": "shopping-list-backend",
  "version": "1.0.0",
  "description": "Backend API for collaborative shopping list app",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
